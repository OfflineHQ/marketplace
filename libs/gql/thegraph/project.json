{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/gql/thegraph/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/gql/thegraph",
        "main": "libs/gql/thegraph/src/index.ts",
        "tsConfig": "libs/gql/thegraph/tsconfig.lib.json",
        "assets": ["libs/gql/thegraph/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/gql/thegraph/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/gql/thegraph"],
      "options": {
        "jestConfig": "libs/gql/thegraph/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}

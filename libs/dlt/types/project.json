{
  "name": "dlt-types",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/dlt/types/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/dlt/types",
        "main": "libs/dlt/types/src/index.ts",
        "tsConfig": "libs/dlt/types/tsconfig.lib.json",
        "assets": ["libs/dlt/types/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/dlt/types/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/dlt/types"],
      "options": {
        "jestConfig": "libs/dlt/types/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}

{
  "name": "gql-user",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/next/gql/user/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/next/gql/user",
        "main": "libs/next/gql/user/src/generated/index.ts",
        "tsConfig": "libs/next/gql/user/tsconfig.lib.json",
        "assets": ["libs/next/gql/user/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/next/gql/user/**/*.{ts, gql, graphql}"]
      }
    },
    "test-hasura": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/next/gql/user"],
      "options": {
        "jestConfig": "libs/next/gql/user/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}

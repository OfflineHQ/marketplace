{
  "name": "nft-thirdweb-organizer",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/nft/thirdweb-organizer/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/nft/thirdweb-organizer",
        "tsConfig": "libs/nft/thirdweb-organizer/tsconfig.lib.json",
        "packageJson": "libs/nft/thirdweb-organizer/package.json",
        "main": "libs/nft/thirdweb-organizer/src/index.ts",
        "assets": ["libs/nft/thirdweb-organizer/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/nft/thirdweb-organizer/jest.config.ts"
      }
    },
    "test-integration": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/nft/thirdweb-organizer/jest.integration.ts",
        "runInBand": true
      }
    }
  },
  "tags": []
}

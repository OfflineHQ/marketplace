{
  "framework": "nextjs",
  "buildCommand": "pnpm npm-run-all lint:all test:all build:back-office --aggregate-output --parallel",
  "outputDirectory": "../../dist/apps/back-office/.next",
  "installCommand": "pnpm install",
  "ignoreCommand": "bash -c '[[ \"$VERCEL_GIT_COMMIT_REF\" = \"main\" ]] || [[ \"$VERCEL_GIT_COMMIT_REF\" = \"staging\" ]] || [[ \"$VERCEL_GIT_PULL_REQUEST_ID\" != \"\" ]] && exit 0 || exit 1'"
}

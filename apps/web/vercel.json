{
  "framework": "nextjs",
  "buildCommand": "pnpm nx build web --prod",
  "outputDirectory": "./dist/apps/web/.next",
  "installCommand": "pnpm install",
  "crons": [
    {
      "path": "./apps/web/crons/handlePendingOrders.ts",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "./apps/web/crons/setRates.ts",
      "schedule": "0 */12 * * *"
    }
  ]
}

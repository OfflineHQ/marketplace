{
  "framework": "nextjs",
  "buildCommand": "pnpm nx build web --prod",
  "outputDirectory": "../../dist/apps/web/.next",
  "installCommand": "pnpm install",
  "ignoreCommand": "bash -c '[ \"$VERCEL_GIT_COMMIT_REF\" = \"main\" ] || [ \"$VERCEL_GIT_COMMIT_REF\" = \"staging\" ] || [[ \"$VERCEL_GIT_COMMIT_MESSAGE\" =~ \"pull request\" ]] && exit 1 || exit 0'",
  "crons": [
    {
      "path": "/apps/web/crons/handlePendingOrders.ts",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/apps/web/crons/setRates.ts",
      "schedule": "0 */12 * * *"
    }
  ]
}

{
  "framework": "nextjs",
  "buildCommand": "pnpm npm-run-all lint:all test:all build:back-office --aggregate-output --parallel",
  "outputDirectory": "../../dist/apps/web/.next",
  "installCommand": "pnpm install",
  "ignoreCommand": "bash ../../tools/scripts/vercel-ignore.sh",
  "crons": [
    {
      "path": "/apps/web/crons/handlePendingOrders.ts",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/apps/web/crons/setRates.ts",
      "schedule": "0 */12 * * *"
    }
  ]
}

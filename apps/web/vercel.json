{
  "framework": "nextjs",
  "name": "marketplace",
  "buildCommand": "cd ../.. && pnpm nx build web --prod",
  "outputDirectory": "../../dist/apps/web/.next",
  "installCommand": "cd ../.. pnpm install",
  "ignoreCommand": "cd ../.. pnpx nx-ignore web",
  "crons": [
    {
      "path": "../../libs/features/cart-cron/src/lib/handlePendingOrders.ts",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "../../libs/next/currency-cache/src/cron/setRates.ts",
      "schedule": "0 */12 * * *"
    }
  ]
}

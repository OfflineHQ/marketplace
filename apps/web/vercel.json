{
  "framework": "nextjs",
  "buildCommand": "pnpm npm-run-all lint:all test:all build:web --aggregate-output --parallel",
  "outputDirectory": "../../dist/apps/web/.next",
  "installCommand": "pnpm install",
  "ignoreCommand": "node -e \"process.exit(['main', 'staging'].includes(process.env.VERCEL_GIT_COMMIT_REF) || process.env.VERCEL_GIT_PULL_REQUEST_ID !== '' ? 0 : 1)\"",
  "crons": [
    {
      "path": "/apps/web/crons/handlePendingOrders.ts",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/apps/web/crons/setRates.ts",
      "schedule": "0 */12 * * *"
    }
  ]
}

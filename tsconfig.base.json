      "@ui/hooks": ["libs/ui/hooks/src/index.ts"],
      "@ui/icons": ["libs/ui/icons/src/index.ts"],
      "@ui/shared": ["libs/ui/shared/src/index.ts"],
      "@ui/theme": ["libs/ui/theme/src/index.ts"],
      "@utils": ["libs/utils/src/index.ts"],
      "@web/*": ["apps/web/*"],
      "@workspace": ["libs/workspace/src/index.ts"]
    }
  },
  "typeRoots": ["types", "node_modules/@types"],
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "tmp"]
}

{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@features/account/api": ["libs/features/account/api/src/index.ts"],
      "@features/appNav/ui": ["libs/features/appNav/ui/src/index.ts"],
      "@features/appNav/ui/server": ["libs/features/appNav/ui/src/server.tsx"],
      "@features/appNav/ui/stories": [
        "libs/features/appNav/ui/src/stories.tsx"
      ],
      "@features/cart": ["libs/features/cart/src/index.ts"],
      "@features/cart/server": ["libs/features/cart/src/server.ts"],
      "@features/cart/types": ["libs/features/cart/src/lib/types"],
      "@features/organizer/event": [
        "libs/features/organizer/event/src/index.ts"
      ],
      "@features/organizer/event/examples": [
        "libs/features/organizer/event/src/examples.tsx"
      ],
      "@features/organizer/event/server": [
        "libs/features/organizer/event/src/server.ts"
      ],
      "@features/organizer/event/store": [
        "libs/features/organizer/event/src/lib/store"
      ],
      "@features/organizer/event/types": [
        "libs/features/organizer/event/src/lib/types"
      ],
      "@features/pass": ["libs/features/pass/src/index.ts"],
      "@features/pass/server": ["libs/features/pass/src/server.ts"],
      "@features/settings": ["libs/features/settings/src/index.ts"],
      "@features/settings/server": ["libs/features/settings/src/server.ts"],
      "@logger": ["libs/logger/src/index.ts"],
      "@next/auth": ["libs/next/auth/src/index.ts"],
      "@next/gql/admin": ["libs/next/gql/admin/src/generated/index.ts"],
      "@next/gql/admin/types": ["libs/next/gql/admin/src/generated/types.d.ts"],
      "@next/gql/user": ["libs/next/gql/user/src/generated/index.ts"],
      "@next/gql/user/types": ["libs/next/gql/user/src/generated/types.d.ts"],
      "@next/hasura/fetcher": ["libs/next/hasura/fetcher/src/index.ts"],
      "@next/hasura/utils": ["libs/next/hasura/utils/src/index.ts"],
      "@next/hygraph": ["libs/next/hygraph/src/index.ts"],
      "@next/hygraph/examples": ["libs/next/hygraph/src/examples.tsx"],
      "@next/hygraph/server": ["libs/next/hygraph/src/server.ts"],
      "@next/i18n": ["libs/next/i18n/src/index.ts"],
      "@next/next-auth/common": ["libs/next/next-auth/common/src/index.ts"],
      "@next/next-auth/options": ["libs/next/next-auth/options/src/index.ts"],
      "@next/react-query": ["libs/next/react-query/src/index.ts"],
      "@next/safe/auth": ["libs/next/safe/auth/src/index.ts"],
      "@next/siwe/provider": ["libs/next/siwe/provider/src/index.ts"],
      "@next/store": ["libs/next/store/src/index.ts"],
      "@next/types": ["libs/next/types/src/index.ts"],
      "@next/zustand": ["libs/next/zustand/src/index.ts"],
      "@server/task": ["libs/server/task/src/index.ts"],

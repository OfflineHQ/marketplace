    "cypress": "cypress",
    "analyze:web": "cross-env ANALYZE=true nx build web",
    "build-storybook:ui": "nx run ui-components:build-storybook",
    "build-storybook:web": "nx run web:build-storybook",
    "vercel-build": "npm i -g node-gyp"
